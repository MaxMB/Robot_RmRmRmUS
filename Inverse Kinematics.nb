Cell[BoxData[{
 RowBox[{
  RowBox[{
   TemplateBox[{"p", "\"S\"", "\"0\""},
    "Subsuperscript"], "=", 
   RowBox[{"{", 
    RowBox[{
     TemplateBox[{"x", "\"S\"", "\"0\""},
      "Subsuperscript"], ",", 
     TemplateBox[{"y", "\"S\"", "\"0\""},
      "Subsuperscript"], ",", 
     TemplateBox[{"z", "\"S\"", "\"0\""},
      "Subsuperscript"]}], "}"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   TemplateBox[{"R", "\"4\"", "\"S\""},
    "Subsuperscript"], "=", 
   RowBox[{
    RowBox[{"Rotation", "[", "\"\<z\>\"", "]"}], "[", 
    RowBox[{
     SubscriptBox["q", "4"], "[", "t", "]"}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   TemplateBox[{"R", "\"5\"", "\"4\""},
    "Subsuperscript"], "=", 
   RowBox[{
    RowBox[{"Rotation", "[", "\"\<x\>\"", "]"}], "[", 
    RowBox[{
     SubscriptBox["q", "5"], "[", "t", "]"}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   TemplateBox[{"p", "\"T\"", "\"5\""},
    "Subsuperscript"], "=", 
   RowBox[{"{", 
    RowBox[{"0", ",", 
     SubscriptBox["l", "B"], ",", "0"}], "}"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   TemplateBox[{"p", "\"T\"", "\"0S\""},
    "Subsuperscript"], "=", 
   RowBox[{
    RowBox[{
     RowBox[{
      TemplateBox[{"R", "\"4\"", "\"S\""},
       "Subsuperscript"], ".", 
      TemplateBox[{"R", "\"5\"", "\"4\""},
       "Subsuperscript"], ".", 
      TemplateBox[{"p", "\"T\"", "\"5\""},
