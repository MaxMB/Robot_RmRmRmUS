         RowBox[{"(", 
           RowBox[{"\[PartialD]", 
             OverscriptBox[
              StyleBox["q", "TI"], "."], "/", "\[PartialD]", 
             StyleBox["u", "TI"]}], ")"}]}], TraditionalForm], "errors" -> {},
